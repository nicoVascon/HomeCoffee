<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HomeCoffee" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">#000F37</item>

        <item name="colorPrimaryVariant">#00BCD4</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#000F37</item>
        <!-- Customize your theme here. -->
        <item name="android:navigationBarColor">#5F71AF</item>


    </style>

    <style name="Theme.HomeCoffee.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.HomeCoffee.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.HomeCoffee.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Button.LightBlue" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">#5F71AF</item>
    </style>
</resources>